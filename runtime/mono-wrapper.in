#! /bin/sh
r='@mono_build_root@'
MONO_CFG_DIR='@mono_cfg_dir@'
PATH="$r/runtime/_tmpinst/bin:$PATH"
MONO_EXECUTABLE=${MONO_EXECUTABLE:-"$r/@mono_runtime@"}
MONO_WRAPPER_EXECUTABLE=${MONO_WRAPPER_EXECUTABLE:-"$MONO_EXECUTABLE"}
export MONO_CFG_DIR PATH
exec "$r/libtool" --mode=execute "${MONO_WRAPPER_EXECUTABLE}" --config "@mono_cfg_dir@/mono/config" "$@"
